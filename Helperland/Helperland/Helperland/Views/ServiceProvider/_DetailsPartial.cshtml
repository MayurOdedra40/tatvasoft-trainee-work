                        }
                    </div>
                    <div class="columnAvatar">
                        @if (UserProfilePicture == "5")
                        {
                            <img class="avatar active" src="~/images/avatar-male.png" data-id="5" onclick="getAvtar(this)">
                        }
                        else
                        {
                            <img class="avatar" src="~/images/avatar-male.png" data-id="5" onclick="getAvtar(this)">
                        }
                    </div>
                    <div class="columnAvatar">
                        @if (UserProfilePicture == "6")
                        {
                            <img class="avatar active" src="~/Images/avatar-ship.png" data-id="6" onclick="getAvtar(this)">
                        }
                        else
                        {
                            <img class="avatar" src="~/Images/avatar-ship.png" data-id="6" onclick="getAvtar(this)">
                        }
                    </div>
                </div>
            </div>

            <div class="form-section">
                <h3 class="title"> My address</h3>
                <hr>
                <div class="row">
                    <div class="col-sm-4">
                        <div class="form-group">
                            <label asp-for="SpAddress.AddressLine1"  class="control-label normal">Street name</label>
                            <input asp-for="SpAddress.AddressLine1" name="SpAddress.AddressLine1"  class="form-control" type="text" placeholder="Street name">
                            <!---->
                        </div>
                    </div>
                    <div class="col-sm-4">
                        <div class="form-group">
                            <label asp-for="SpAddress.AddressLine2" class="control-label normal">House number</label>
                            <input asp-for="SpAddress.AddressLine2" name="SpAddress.AddressLine2" class="form-control" type="text" placeholder="House number">
                            <!---->
                        </div>
                    </div>
                    <div class="col-sm-4">
                        <div class="form-group">
                            <label asp-for="ServieProvider.ZipCode" class="control-label normal">Postal code</label>
                            <input asp-for="ServieProvider.ZipCode" name="ServieProvider.ZipCode" class="form-control" type="text" placeholder="Postal code" onchange="getCity(this)">
                            <span asp-validation-for="ServieProvider.ZipCode" class="danger">
                            </span>
                            <span id="forWrongZipcode" class="danger"></span>

                        </div>
                    </div>
                    <div class="col-sm-4">
                        <div class="form-group">
                            <label asp-for="SpAddress.City" class="control-label normal">City</label>
                            <select asp-for="SpAddress.City"  name="SpAddress.City" class="form-control" id="CitySelect">
                                <option value="@Model.SpAddress.City">@Model.SpAddress.City</option>
                            </select>
                        </div>
                    </div>
                </div>
            </div>
            <div class="form-group">
                <button class="btn btn-primary">Save</button>
            </div>
        </div>
    </div>
</form>