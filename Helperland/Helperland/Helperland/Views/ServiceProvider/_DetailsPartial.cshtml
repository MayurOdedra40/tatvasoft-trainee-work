@*@model User*@

@model SpViewModel
@{ 

    string Email = Model.ServieProvider.Email;
    string UserProfilePicture = Model.ServieProvider.UserProfilePicture;
}
<h3 class="account-status">
    Account Status:
    <span>Active</span>
    @if (UserProfilePicture != null)
    {
        if (UserProfilePicture == "1")
        {
            <img id="spAvtarDefault" alt="avtar" style="float: right;" src="~/images/avatar-car.png">
        }
        else if (UserProfilePicture == "2")
        {
            <img id="spAvtarDefault" alt="avtar" style="float: right;" src="~/images/avatar-female.png">
        }
        else if(UserProfilePicture == "3")
        {
            <img id="spAvtarDefault" alt="avtar" style="float: right;" src="~/images/avatar-hat.png">
        }
        else if(UserProfilePicture == "4")
        {
            <img id="spAvtarDefault" alt="avtar" style="float: right;" src="~/images/avatar-iron.png">
        }
        else if(UserProfilePicture == "5")
        {
            <img id="spAvtarDefault" alt="avtar" style="float: right;" src="~/images/avatar-male.png">
        }
        else if(UserProfilePicture == "6")
        {
            <img id="spAvtarDefault" alt="avtar" style="float: right;" src="~/images/avatar-ship.png">
        }


    }
        <img id="spAvtar1" class="av d-none" alt="avtar" style="float: right;" src="~/images/avatar-car.png">
        <img id="spAvtar2" class="av d-none" alt="avtar" style="float: right;" src="~/images/avatar-female.png">
