</head>
<body class="">

    @RenderSection("section1")

    <partial name="_navbarPartial" />

    @RenderBody()

    <partial name="_footerPartial" />

    @RenderSection("cookies", required: false)

    <partial name="_Scripts" />

    <script>
        $(function () {
            $(document).scroll(function () {
                var $nav = $("#service-header");
                $nav.toggleClass('scrolled', $(this).scrollTop() > 1);
            });
        });
    </script>

    @if (TempData.ContainsKey("ModalName"))
    {
        <script>

            $(document).ready(function () {
                var md = '@TempData["ModalName"]';
                $(md).modal('show');

            });
        </script>
    }

    @if (TempData.ContainsKey("Modal2Name"))
    {
        <script>

            $(document).ready(function () {
                var md = '@TempData["Modal2Name"]';
                $(md).modal('show');

            });
        </script>
    }


    @RenderSection("LogoutScript", required: false)
</body>

</html>